[sub_resource type="GDScript" id=4]

script/source = "extends TextureButton

onready var settings = preload(\"res://scenes/settings.tscn\")

func _on_menu_pressed():
	if not g.stage.has_node(\"hud/settings\"):
		var window = settings.instance()
		window.setup(g.music_on, g.sound_on, cfg.SETTINGS_WINDOW_POS)
		g.stage.get_node(\"hud\").add_child(window)
		get_tree().set_pause(true)
"

